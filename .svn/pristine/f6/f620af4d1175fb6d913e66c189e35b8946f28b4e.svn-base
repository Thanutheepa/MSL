        if (post == "Pirith" || post == "Mother-Sri-Lanka-Flagship-Showroom-Opening" || post == "THALA-MSL-DAY-2019–Nikaweratiya Mahasen National School" ||
            post == "මදර්ශ්‍රී-ලංකා-මාතෘ-භුමිය-වෙනුවෙන්-කරනු-ලබන-නිහඬ-සේවයේ-දසවන-සංවත්සරය-සමරයි-තරුණ-නායකත්වය-සහ-සහජීවන-කඳවුර-සහ-දේශීය-නිපැයුම්-වෙළඳසැල" || post == "Mother-Sri-Lanka-celebrates-10-years-of-silent-service-to-the-country-Youth-Leadership-and-Harmony-Ambassador-Camp-Authentic-Sri-Lankan-Products-Gallery")
        {
            imgfiles = GetAllImages();
        }

    }

    private List<ImageSpec> GetAllImages()
    {
        List<ImageSpec> result = new List<ImageSpec>();
        string imgPath = @"\images\News\";

        if (post == "Pirith")
        {
            item = "pirith";
        }

        if (post == "Mother-Sri-Lanka-Flagship-Showroom-Opening")
        {
            item = "Flagship Showroom Opening";
        }

        if (post == "THALA-MSL-DAY-2019–Nikaweratiya Mahasen National School")
        {
            item = "THALA- MSL DAY";
        }

        if (post == "මදර්ශ්‍රී-ලංකා-මාතෘ-භුමිය-වෙනුවෙන්-කරනු-ලබන-නිහඬ-සේවයේ-දසවන-සංවත්සරය-සමරයි-තරුණ-නායකත්වය-සහ-සහජීවන-කඳවුර-සහ-දේශීය-නිපැයුම්-වෙළඳසැල" || post == "Mother-Sri-Lanka-celebrates-10-years-of-silent-service-to-the-country-Youth-Leadership-and-Harmony-Ambassador-Camp-Authentic-Sri-Lankan-Products-Gallery")
        {
            item = "නිහඬ සේවයේ දසවන සංවත්සරය";
        }

        DirectoryInfo d = new DirectoryInfo(@"wwwroot\images\News\" + item);
        var files = d.GetFiles("*.*");
        if (files != null)
        {
            foreach (var f in files)
            {
                if (f.Name.Contains("300x300"))
                {
                    continue;
                }
                else
                {
                    var spec = new ImageSpec
                    {
                        Name = Path.GetFileNameWithoutExtension(f.Name),
                        DisplayURL = $"{imgPath}\\{item}\\{f.Name}"
                    };
                    result.Add(spec);
                }

            }
        }
        return result;
    }


    public class ImageSpec
    {
        public string Name { get; set; }
        public string DisplayURL { get; set; }
    }

